{
  "name": "psr",
  "version": "1.0.0",
  "description": "Personal sport record application",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build": "npm run clean && NODE_ENV=production webpack -p",
    "build:dev": "npm run clean && webpack",
    "compile-mq": "node-sass src/scss/foundation_mq.scss dist/css/foundation_mq.css",
    "dev": "cp config.dev.js config.js && npm run compile-mq && webpack-dev-server --progress --colors",
    "ngrok": "NODE_ENV=production webpack -p --watch",
    "server": "npm run ngrok & cp dist/index.html ../server/index.twig & node ../server/index.js",
    "deploy": "cp config.prod.js config.js && npm run build && AWS_DEFAULT_PROFILE=cam aws s3 cp dist s3://web.personalsportrecord.com --recursive && cp config.dev.js config.js",
    "deploy:dev": "cp config.dev.js config.js && npm run build:dev && AWS_DEFAULT_PROFILE=cam aws s3 cp dist s3://dev.personalsportrecord.com --recursive",
    "deploy:clone": "cp config.clone.js config.js && npm run build && AWS_DEFAULT_PROFILE=cam aws s3 cp dist s3://clone.personalsportrecord.com --recursive && cp config.dev.js config.js",
    "deploy:staging": "cp config.staging.js config.js && npm run build && AWS_DEFAULT_PROFILE=cam aws s3 cp dist s3://staging.personalsportrecord.com --recursive && cp config.dev.js config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "blueimp-load-image": "^2.13.1",
    "file-loader": "^0.11.1",
    "foundation-sites": "^6.3.1",
    "get-video-id": "^2.1.6",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "moment": "^2.18.1",
    "motion-ui": "^1.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^1.1.3",
    "recharts": "^1.0.0-beta.10",
    "rxjs": "^5.3.1",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.2",
    "@types/node": "^10.1.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-router": "^4.0.25",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "1.1.3",
    "@types/recharts": "^1.0.21",
    "@types/stripe": "^5.0.14",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.17",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^6.0.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "ts-loader": "3.5.0",
    "typescript": "^2.8.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
